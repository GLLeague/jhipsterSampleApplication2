entity Player {
	playerName String,
    nickname String,
    playerId Integer
}

entity Team {
	teamName String,
    id Integer
}

entity Match {
	id Long,
	teamOne Team,
    teamTwo Team,
    type String
}

enum MatchType {
	TOURNAMENT,
    CLASSIC
}

entity MatchEvent {
	player Player,
    type String
}

enum EventType {
	GOAL,
	ASSIST    
}

entity Round {
	order Integer,
    id Long
}

entity Season {
	name String,
    id Long
}


// Set an angular suffix
angularSuffix * with mySuffix
